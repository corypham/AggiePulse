{
  "expo": {
    "name": "AggiePulse",
    "slug": "aggiepulse",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aggiepulse",
      "config": {
        "googleMapsApiKey": "AIzaSyDVWzOZH-PdI0517aXjpcKnHDubvfwHSF4"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aggiepulse",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCYiF-72X_YTsJ96JyDPzlCKQS0bGN-mXk"
        }
      }
    },
    "web": {
      "bundler": "metro"
    },
    "newArchEnabled": true,
    "plugins": [
      "expo-router",
      "expo-asset",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow AggiePulse to use your location."
        }
      ],
      "./plugins/AndroidIOSApiKey.js"
    ],
    "scheme": "aggiepulse"
  }
}
